package loops;

public class Fifth {

	public static void main(String[] args) {
		
		
		//1=> 100000=> 
		double sum=0;
		
		for(double num=1.0; num<=100000; num++)
		{
			if(num%13.5==0)
			{
				System.out.println(num);
				sum=sum+num;
				// sum+=num;
				System.out.println("*");
			}
		}
		System.out.println(sum);

	}

}
