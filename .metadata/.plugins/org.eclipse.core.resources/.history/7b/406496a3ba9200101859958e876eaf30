package loops;

public class Fourth {
	public static void main(String[] args) {
		
		
		
		
		  // 1=> 374 => even numbers =>(num %2==0)
		// ICBO
		
		int sum=0;//0+1+2+3+4+5
		
		// 1=> 50 => Summation
		   for(int num=1;num <=5;num++)
		   {
			   sum=sum+num;
		   }
		   System.out.println(sum);
		

	}

}
