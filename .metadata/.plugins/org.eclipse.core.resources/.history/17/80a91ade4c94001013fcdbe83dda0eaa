package conditional;

public class Days {

	public static void main(String[] args) {
		
		String day="Thursday"; //today's day
		
		switch(day)
		{
		case "Friday":
		{
			System.out.println("Today is friday.....");
		}
		case "Thursday":
		{
			System.out.println("today is Thursday..");
		}
		}
	}

}
