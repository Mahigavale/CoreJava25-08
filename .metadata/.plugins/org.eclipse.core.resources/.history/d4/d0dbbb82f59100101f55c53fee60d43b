package loops;

public class First {

	public static void main(String[] args) {
		
		
	
		// Loops=> doing one thing again and again. till the specific time/condition.
		 //for.
		//enhanced for.
		
		
		
		//for=> variable, condition , operation on variable.\
		// variable initialization , condition checking , variable updation / operation
		
		
		
		
		
		
//		if() //true or false
//		{
//			
//		}
//		
//		else if() // true or false
//		{
//			
//		}
		
		
		
// 1)Variable init.	
// 2) Condition for variable.	 i=>19
// 3) Operation on the variable.		
		
		int i=10;
		System.out.println(10+5);
		System.out.println(i++);
		
		for(int i=10;
				i<20;
				i++)   // ++,-- => Unary => increment and decrement
		{
		}
		
		
	}

	
	/**
	 * Unreachable
	 */
}




