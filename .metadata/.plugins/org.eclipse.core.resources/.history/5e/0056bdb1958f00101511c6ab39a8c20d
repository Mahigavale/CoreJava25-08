package conditional;

public class Ladder {

	public static void main(String[] args) {
		
		double marks=46;
		
		//&& => used to introduce the range.
		if(marks >=35 && marks <=45) //35 => 45
		{
			System.out.println("Third divison.");
		}
		else if(marks >45  && marks <=60) //46=> 60
		{
			System.out.println("Second divison");
		}
		else if(marks >60 &&  marks <=75) //61=>75
		{
			System.out.println("first divison");
		}
		else if(marks >75 && marks <=100)
		{
			System.out.println("Distinction.");
		}
	}

}
