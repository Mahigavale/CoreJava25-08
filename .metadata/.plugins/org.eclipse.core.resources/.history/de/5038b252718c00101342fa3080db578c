package operators;

public class Shorthand {

	public static void main(String[] args) {
		
		
	
		int age=23; //11a
		
		  age+=5; //11a=11a+5;  age=age+5;
		  age*=2;   // age=age*2;
		  
		  age/=4;   //age=age/4;
		 //System.out.println(age); //11a
		 
		   age-=13;
		 System.out.println(age+=3);
		 
		 //System.out.println(age=age+3);
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		 
		
		 
		 
		 
		 
		 
		 
		 
		 age+=124;  //128
		 
		 age%=4;
		 System.out.println(age);
	}

}
