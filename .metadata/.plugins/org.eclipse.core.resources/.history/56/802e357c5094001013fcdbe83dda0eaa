package conditional;

public class TransferStatements {

	public static void main(String[] args) {
		// Break;=>
		//continue;=>
		
		for(int i=0;i<20;i++)
		{
			
			if(i%2==0)
			{
				//break; => terminate the whole loop
				continue; //skip the current iteration.
			}
			System.out.print(i+" ");
		}
	}

}
