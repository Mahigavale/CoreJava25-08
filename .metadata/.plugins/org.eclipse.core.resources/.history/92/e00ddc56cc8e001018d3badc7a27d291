package conditional;

public class ifblock {

	public static void main(String[] args) {
		
		int num=100; //100
		int num2=007; //7
		
		if(num2>num) //false
		{
			System.out.println("hello");
		}
		
		else
		{
			 System.out.println("not hello");
		}
		
		
		System.out.println("Program done>>>>");
		
		
	}

}
