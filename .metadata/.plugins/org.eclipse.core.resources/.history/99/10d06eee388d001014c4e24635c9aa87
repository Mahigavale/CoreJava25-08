package operators;

public class Shorthand2 {

	public static void main(String[] args) {
		
		int num=100; //= => assign
		int num2=101;
	//	System.out.println(num==num2); //== equality
		
		
		 //% 
		//10/3=>3
		//10%3=>1
		
		
		//System.out.println(100<=100);
		num+=5; // num+5=num
		System.out.println(num);
		

	}

}
