package datatypes;

public class Nonprimitive {

	public static void main(String[] args) {
		
		//Non-Primitive:
		// size not fixed.
		//stored inside Heap.
		
		//Array=> Collection of similar data type.
		//Linear DS=>[][][][][][][]
		//int num=null;
		System.out.println();
		
		int[] arr=new int[10];   //4 bytes
		arr[0]=10;
		arr[1]=20;
		arr[2]=30;
		arr[3]=40;
		arr[4]=50;
		
		System.out.println(arr.length);
	}

}
