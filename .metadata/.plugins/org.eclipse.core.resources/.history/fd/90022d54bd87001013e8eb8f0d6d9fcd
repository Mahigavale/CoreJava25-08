package datatypes;

public class Casting {

	public static void main(String[] args) {
		
		//narrowing , explicit type casting
		 double d=12.45; //football
		 System.out.println(d);
		  int i=(int)d;    //cricket
		  System.out.println(i);
		  // Larger=> smaller => data Lost.
		  
		  
		  
		  int age=30;

	}

}
