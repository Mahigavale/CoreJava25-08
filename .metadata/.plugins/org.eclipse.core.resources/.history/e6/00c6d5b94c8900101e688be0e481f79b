package operators;

public class Arithmetic {

	public static void main(String[] args) {
	//arithmetic 
		//Plus
		 // increment=> Pre and Post
		
		int num=10;
	
	//	System.out.println(num1);
		
	//	System.out.println((num++)+10);
	//	System.out.println(num);
		
		//System.out.println(num);
		
		
		int  age=10;
		       ++age;  //11
		System.out.println(++age+(age++)+(++age)+(age++));
                           //12+12+14+14
	}

}
