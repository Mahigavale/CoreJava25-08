package datatypes;

public class Nonprimitive {

	public static void main(String[] args) {
		
		//Non-Primitive:
		// size not fixed.
		//stored inside Heap.
		
		//Array=> Collection of similar data type.
		//Linear DS=>[][][][][][][]
		//int num=null;
		System.out.println();
		
		int[] arr=new int[10];   //4 bytes
		arr[0]=10;
		arr[1]=20;
		arr[2]=30;
		arr[3]=40;
		arr[4]=50;
		
		System.out.println(arr.length);
		System.out.println("Third Space:"+arr[2]);
		               
		System.out.println(arr);//0,1,2=> Third space number
		
		
		char[] letters=new char[3];
		
		letters[0]='a';
		letters[1]='b';
		letters[2]='c';
		
		
	
		
		System.out.println(letters[0]);
		
		System.out.println(letters.hashCode());
		
		
		
	}

}
