package conditional;

public class Ladder {

	public static void main(String[] args) {
		
		double marks=300;
		
		//&& => used to introduce the range.
		if(marks >=35 && marks <=45)
		{
			System.out.println("Third divison.");
		}
		else if(marks >45  && marks <=60)
		{
			System.out.println("Second divison");
		}
	}

}
